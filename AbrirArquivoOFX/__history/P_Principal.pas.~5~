{$R *.dfm}

procedure TForm1.Button1Click(Sender: TObject);
var
  YMOFXReader1: TYMOFXReader;
  i:integer;
begin
  YMOFXReader1:= TYMOFXReader.create(self);
  YMOFXReader1.OFXFile := 'D:\Delphi\TrabalhoAlterdata\ExtratoOFX.ofx';
  YMOFXReader1.Process;
  ListBox1.Clear;
//  for i := 0 to YMOFXReader1.Count-1 do
//  begin
//    ListBox1.Items.Add(YMOFXReader1.Get(i).Desc + ' ' +
//                       FloatToStr(YMOFXReader1.Get(i).Value)// + ' ' +
//                       YMOFXReader1.Get(i).MovType + ' ' +
//                       dateToStr(YMOFXReader1.Get(i).MovDate) + ' ' +
//                       YMOFXReader1.Get(i).ID + ' ' +
//                       YMOFXReader1.Get(i).Document + ' ');

//DBGrid1.Columns[1].FieldName:= FloatToStr(YMOFXReader1.Get(i).Value);
//  end;
  for i := 1 to YMOFXReader1.Count-1 do
  begin
    StringGrid1.Cells[1,i] := IntToStr(i);
    StringGrid1.Cells[2,i] := YMOFXReader1.Get(i).Desc;
    StringGrid1.Cells[3,i] := FloatToStr(YMOFXReader1.Get(i).Value);

  end;

end;
procedure TForm1.FormCreate(Sender: TObject);
begin
StringGrid1.Cells[0,0] := 'Descrição';
StringGrid1.Cells[1,0] := 'Valor';

end;

end.
